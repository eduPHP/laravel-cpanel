{
    "name": "swalker2/cpanel",
    "description": "the simplest PHP implementation of the cpanel versio 2 api for Laravel",
    "license": "MIT",
    "keywords": ["laravel","cpanel"],
    "authors": [
        {
            "name": "swalker2",
            "email": "edu@rdo.blog.br"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "^5.4",
        "guzzlehttp/guzzle": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Swalker2\\Cpanel\\": "src/"
        }
    },
    "autoload-dev":{
       "files": [
           "tests/mocks/config.php",
           "tests/mocks/request.php",
           "tests/ZoneEdit/MockResponses.php",
           "tests/Email/MockResponses.php"
       ]
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "vlucas/phpdotenv": "^2.4"
    }
}
